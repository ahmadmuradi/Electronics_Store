{
  "name": "electronics-store-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --config=jest-preset.js --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "prebuild": "expo prebuild --clean",
    "build": "eas build --platform all --profile preview",
    "build:android": "eas build --platform android --profile preview",
    "build:ios": "eas build --platform ios --profile preview",
    "build:staging": "eas build --platform all --profile staging",
    "build:production": "eas build --platform all --profile production",
    "build:status": "eas build:list --limit=5",
    "build:cancel": "eas build:cancel",
    "clean": "rimraf node_modules && rimraf package-lock.json && npm install",
    "clean:build": "rimraf android ios web dist build",
    "clean:cache": "expo start -c && npm cache clean --force",
    "clean:all": "npm run clean && npm run clean:build && npm run clean:cache",
    "bump-version": "bump --tag --push --all",
    "release": "npm-run-all --sequential build:production publish:ota",
    "publish:ota": "eas update --branch production --message \"$npm_package_version\" --auto",
    "publish:staging": "eas update --branch staging --message \"$npm_package_version\" --auto",
    "verify:deps": "node -e \"const pkg = require('./package.json'); Object.keys(pkg.dependencies).forEach(dep => { try { require.resolve(dep); console.log('✅ ' + dep); } catch(e) { console.log('❌ ' + dep); } });\""
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.1",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-community/netinfo": "^11.1.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.9.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-native": "^5.0.0",
    "expo": "~50.0.0",
    "expo-background-fetch": "~11.8.0",
    "expo-barcode-scanner": "~12.9.0",
    "expo-camera": "~14.1.3",
    "expo-constants": "^18.0.10",
    "expo-font": "^14.0.9",
    "expo-notifications": "^0.32.12",
    "expo-router": "~3.4.10",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "^0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-task-manager": "~11.8.0",
    "graphql": "^16.11.0",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.6.2",
    "react-native-screens": "~3.29.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "sentry-expo": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^7.0.0",
    "jest": "^29.5.0",
    "jest-expo": "^49.0.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^2.4.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-safe-area-view": "^2.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.20.8"
  },
  "private": true,
  "description": "Electronics Store Mobile Inventory Management App",
  "author": "Electronics Store Team",
  "license": "MIT"
}
